module libanalysis2/prelude/term

rules
  
  tapp(s|c): c#([a]) -> c#([<s>a])
  tapp(s1,s2|c): c#([a1,a2]) -> c#([<s1>a1,<s2>a2])
  tapp(s1,s2,s3|c): c#([a1,a2,a3]) -> c#([<s1>a1,<s2>a2,<s3>a3])
  tapp(s1,s2,s3,s4|c): c#([a1,a2,a3,a4]) -> c#([<s1>a1,<s2>a2,<s3>a3,<s4>a4])
  tapp(s1,s2,s3,s4,s5|c): c#([a1,a2,a3,a4,a5]) -> c#([<s1>a1,<s2>a2,<s3>a3,<s4>a4,<s5>a5])
