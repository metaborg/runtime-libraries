module libanalysis2/pp/preload

imports
  libstratego-gpp
  runtime/tmpl/pp

// These rules should be applied last, so the need to be loaded as
// early as possible.

rules

  prettyprint-Term: t@c#(a*) -> [ H( [SOpt(HS(), "0")],
                                     [S(c),S("["),b,S("]")] ) ]
  where b := <pp-H-list(prettyprint-Term|",")> a*