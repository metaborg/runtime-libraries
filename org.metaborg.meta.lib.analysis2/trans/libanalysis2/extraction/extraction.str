module libanalysis2/extraction/extraction

imports
  signatures/libanalysis2/-

signature
  constructors
    Cs : List(Cs) -> C

rules
 
  do-extract-constraint = fail

  extract-constraints =
    do-extract-constraint
  ; (id,constraint-to-list;constraints-nontrivial)

  constraint-to-list = 
     ?CAndStar(<constraint-to-list>) 
  <+ mapconcat(constraint-to-list) 
  <+ ![<id>]
  
  constraints-nontrivial = remove-all(?CTrue())
