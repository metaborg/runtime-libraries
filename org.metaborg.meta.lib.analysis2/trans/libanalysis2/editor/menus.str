module libanalysis2/editor/menus

imports

  libanalysis2/-
  libanalysis2/export/-

rules

  show-analysis: (_, _, ast, path, project-path) -> (filename, result)
    with filename := <guarantee-extension(|"analysis2")> path
       ; result := <get-analysis;pp-analysis> ast

  show-analysis-aterm: (_, _, ast, path, project-path) -> (filename, result)
    with filename := <guarantee-extension(|"analysis2.aterm")> path
       ; result := <get-analysis> ast
